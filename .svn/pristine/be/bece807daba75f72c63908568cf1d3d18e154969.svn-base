/**
 请求成功block
 */
typedef void (^requestSuccessBlock)(id responseObj);

/**
 请求失败block
 */
typedef void (^requestFailureBlock) (NSError *error);


#ifdef DEBUG
#define KSMLog(...) NSLog(__VA_ARGS__)
#else
#define KSMLog(...)
#endif

