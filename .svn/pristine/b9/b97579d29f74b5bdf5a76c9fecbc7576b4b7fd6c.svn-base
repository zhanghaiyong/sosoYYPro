@interface NSArray (XTExtension)

/**
 *  将数组前面几个元素作为一个数组返回
 */
- (NSArray *)head:(NSUInteger)count;
/**
 *  将数组后面几个元素作为一个数组返回
 */
- (NSArray *)tail:(NSUInteger)count;

@end


@interface NSArray (XTJson)

//将NSArray转化成NSData
- (NSData *)JSONData;
//将NSArray转化成NSString
- (NSString *)JSONString;

@end
